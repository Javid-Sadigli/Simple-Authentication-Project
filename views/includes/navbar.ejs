<header class="page_header">
    <a class="navbar_logo_a" href="/">
        <h1 class="navbar_logo">
            Auth Project
        </h1>
    </a>
    <% if (!logged_in) { %>
        <nav>
            <ul class="navbar_list">
                <li><a href="/register">Sign up</a></li>
                <li><a href="/login">Log in</a></li>
            </ul>
        </nav>
    <% } else {%>
        <a class="profile_icon_a" href="/profile">
            <div class="profile_icon">
                <img src="/assets/icons/user_icon.png" alt="profile">
            </div>
        </a>
    <% } %>
</header>